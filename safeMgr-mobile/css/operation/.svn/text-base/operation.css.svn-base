.card {
    color: #555555;
    font-size: 18px;
    width: 100%;
    margin: auto;
    margin-bottom: 10px;
}

.mui-table-view {
    width: 98%;
    margin: auto;
}

#confirm,
#confirmtr {
    width: 90%;
    float: left;
}

#confirm,
#confirmtr {
    margin-right: 10%;
    margin-left: 5%;
}

.mui-media-body span {
    color: white;
    font-size: 16px;
}

.mui-media-body {
    color: white;
}

.op-mui {
    font-size: 16px;
    margin-left: 0;
    color: white;
}

.mui-input-row label {
    width: 30%;
}

#showUserPicker {
    border: none;
    width: 50%;
    margin-left: 0px;
    font-size: 18px;
    text-align: left;
}

#operate-list {
    width: 95%;
    text-align: left;
    margin: auto;
}

table {
    width: 100%;
}

table tr td {
    text-align: center;
    color: #5ca4d7
}

.op-button {
    border-color: #2fb2f0;
    color: #2fb2f0;
    width: 98%;
    margin: 10px auto;
}

.op-list {
    width: 100%;
    margin: 5px auto;
    background: white;
}

#sg_detail {
    width: 95%;
    margin: 3px auto;
}

#xq-button button {
    width: 95%;
    margin: 3px auto;
}

#sgqk {
    height: 40px;
    overflow: hidden;
    color: #797979;
}

.text_show {
    margin-top: 4px;
    background-color: #5ca4d7;
    color: white;
    font-size: 16px;
    width: 100%;
}

#sgfsdd {
    margin-right: 30%;
}

.mui-page-content {
    margin-top: 50px;
    width: 100%;
}

.mui-table-view {
    width: 95%;
}

#page_li {
    background: #5ca4d7;
    border: none;
}


/*弹框*/

.mui-popover {
    height: 200px;
    width: 97%;
    margin: auto;
}

#middlePopover ul li {
    width: 100%;
}

#middlePopover ul label {
    color: #333333;
    width: 30%;
}

#popover span {
    font-size: 16px;
}


/*错误图片*/

#div-error img {
    margin-top: 20%;
    width: 30%;
}

#div-error p {
    margin-top: 40px;
    font-size: 20px;
}

.sound-close {
    position: absolute;
    display: inline-block;
    right: 10px;
    top: 10px;
    width: 20px;
    height: 20px;
    text-align: center;
    line-height: 20px;
    border-radius: 12px;
    background-color: #FF5053;
    color: #f3f3f3;
    border: solid 1px #FF5053;
    font-size: 9px;
    font-weight: 200;
    z-index: 1;
}

#detail {
    width: 95%;
}