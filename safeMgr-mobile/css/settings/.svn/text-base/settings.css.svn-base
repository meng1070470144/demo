html,
body {
    background: white;
    font-size: 18px;
    background: #EFEFF4;
}

ul li img {
    height: 40px;
    width: 40px;
    margin-left: 5px;
    margin-right: 10px;
    float: left;
}

ul li span a {
    vertical-align: middle;
    color: #929292;
}

.item {
    margin-top: 16px;
    color: #555555;
    font-size: 20px;
}

.item-exit {
    margin: 10px 125px;
    color: #DD524D;
}

.mui-table-view-cell {
    line-height: 10px;
    vertical-align: top;
}

.header-info {
    width: 100%;
    height: 180px;
    background: url(../images/main_background.jpg) no-repeat;
    background-size: 100% 100%;
    text-align: center;
    margin-bottom: 1px;
}

#personlogs {
    width: 100px;
    height: 100px;
    border-radius: 100px;
    margin: auto;
    margin-top: 30px;
    margin-bottom: 10px;
}

.person-list {
    margin-top: 20px;
    border-radius: 10px;
    margin: auto;
}

#person-realName {
    margin-right: 5px;
}

.person-info {
    color: white;
    font-size: 20px;
}